{
  "$schema": "http://json-schema.org/draft-07/schema",
  "required": [
    "sellerName",
    "addonName",
    "addonVersion",
    "upboundNamespace",
    "clusters"
  ],
  "additionalProperties": true,
  "properties": {
    "sellerName": {
      "type": "string"
    },
    "addonName": {
      "type": "string"
    },
    "addonVersion": {
      "type": "string"
    },
    "upboundNamespace": {
      "type": "string"
    },
    "repoUrl": {
      "type": "string"
    },
    "gitRepository": {
      "type": "string"
    },
    "clusters": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}
